<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_start">

    <fragment
        android:id="@+id/navigation_start"
        android:name="com.wnexample.a0909_bottom_nav2.ui.start.StartFragment"
        android:label="Start"
        tools:layout="@layout/fragment_start" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.wnexample.a0909_bottom_nav2.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.wnexample.a0909_bottom_nav2.ui.setting.SettingFragment"
        android:label="Setting"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.wnexample.a0909_bottom_nav2.ui.dashboard.DashboardFragment"
        android:label="Add"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.wnexample.a0909_bottom_nav2.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>